<div class="row full imprints-container">
  <% job.imprints.each do |imprint| %>
    <%= render partial: 'imprints/imprint',
        locals: {
          job: job,
          imprint_method: imprint.imprint_method,
          imprint: imprint,
          description_name: "imprint[#{imprint.id}][description]"
    } %>
  <% end %>
</div>
