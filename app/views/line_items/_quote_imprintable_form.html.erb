<%= form_for [quote, line_item], builder: LancengFormBuilder,
             html: { id: 'new_imprintable_line_item' },
             url: quote_line_items_path(quote, line_item) do |f| %>
  <div class="select-level", data-level="1">
    <div class="select-level", data-level="2">
      <div class="select-level", data-level="3">
        <div class="select-level", data-level="4">

        </div>
      </div>
    </div>
  </div>

  <%= f.hidden_field :imprintable_variant_id %>
<% end %>