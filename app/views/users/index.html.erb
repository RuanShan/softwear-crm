<h1>Users</h1>
<span>(<strong><%= link_to 'Create new user', new_user_registration_path %></strong>)</span>
<div class='box-info full'>
  <div class='table-responsive'>
    <table class='table table-hover'>
      <thead>
      <tr>
        <th style='width: 30px'>ID</th>
        <th>Full Name</th>
        <th>Email Address</th>
        <th>Actions</th>
      </tr>
      </thead>

      <tbody>
        <% @users.each do |user| %>
          <%= render partial: 'table_entry', locals: { user: user } %>
        <% end %>
      </tbody>
    </table>
  </div>
</div>