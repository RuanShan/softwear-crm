<% if @artwork_request.valid? %>
    $('.artwork-request-list').html("<%= escape_javascript(render(partial: 'artwork_request', locals: {artwork_request: @artwork_request, order: @order})).html_safe %>");
    setupContentModal(function($contentModal){
        $contentModal.find('.modal-content').addClass('modal-content-success')
    })
    showContentModal({
        title: 'SUCCESS',
        body: 'Successfuly created Artwork Request!'
    }).delay(50)
<% else %>
    $('#contentModal .modal-body').html("<%= escape_javascript(render(partial: 'new', locals: {artwork_request: @artwork_request, order: @order})).html_safe %>");
    styleCheckboxes();
    initializeSummernote();
    initializeDateTimePicker();
    initializeJobsChosen();
    summernoteArtworkRequest();
    setNewArtworkRequestSelect();
//    $('#errorsModal').modal('show');
<% end %>
